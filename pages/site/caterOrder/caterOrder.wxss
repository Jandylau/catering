page{
  background-color: #f5f5f5;
  height: 100%
}
.margin20{
  margin-top:20rpx
}
.marginBotom{
  margin-bottom:20rpx
}
.caterTop{
  height: 80rpx;
  line-height:80rpx;
  padding-left:30rpx;
  font-size:28rpx
}
.caterCont{
  background-color: #fff;
  border-bottom: 1rpx solid #dedede;
  border-top:1rpx solid #dedede;
  width: 100%;
}
.caterCont .caterTable{
  font-size: 28rpx;
  color:#333;
  width:92%;
  margin:0 auto
}
 .caterTr,.caterRedun{
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom:1rpx solid #dedede;
}
.caterTr{
 padding:20rpx 0;
}
.caterRedun{
   padding:30rpx 0;
}
.caterImg{
  width:100rpx;
  text-align: center;
}
.caterImg image{
  width: 100rpx;
  height: 100rpx;
}
 .caterTitle{
 width:50%;
 padding-left:15rpx;
   overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis; 
}
.hidden{
  display:none
}
.caterNum{
 width:12%;
 text-align: center;  
}
.caterPrice{
   width:18%;
   text-align: right
}
.caterBottom{
  padding:25rpx 30rpx;
  text-align:right;
  font-size:28rpx;
}
.caterBottom label{
  margin-left:20rpx
}
.reduction{
 width:88%; 
}
.fontRed{
  color:#fb4e44
}
.rules{
  font-size:24rpx
}
.payBtn{
position: relative;
 top:130rpx;
 width:80%;
 margin: 0 auto;
 font-size: 32rpx;
 color:#fff;
 text-align: center;
 padding:30rpx 0 ;
 background-color:#ff9900;
 border-radius:16rpx
}
.desk{
 width:200rpx;
 height:88rpx;
 text-align:center;
 font-size:24rpx;
 line-height:88rpx;
 border:1rpx solid #ccc;
 margin:0 auto;
 border-radius:16rpx
}
.noSqcode{
padding:30rpx 0
}